<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Options.
    -->
    <changeSet id="20170525235446-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="value_id"
                                 baseTableName="options"
                                 constraintName="fk_options_value_id"
                                 referencedColumnNames="id"
                                 referencedTableName="value"/>

        <addForeignKeyConstraint baseColumnNames="type_id"
                                 baseTableName="options"
                                 constraintName="fk_options_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="type"/>

        <addForeignKeyConstraint baseColumnNames="sub_category_id"
                                 baseTableName="options"
                                 constraintName="fk_options_sub_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sub_category"/>

    </changeSet>
</databaseChangeLog>
